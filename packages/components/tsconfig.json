{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "jsx": "react",
    "module": "ESNext",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./",
    "sourceMap": true,
    "target": "es5",
    "lib": [
      "dom",
      "es2015",
      "es5",
      "scripthost"
    ],
  },
  "exclude": [
    "node_modules"
  ],    
}
